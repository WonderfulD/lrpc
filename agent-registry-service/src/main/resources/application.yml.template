server:
  port: 18080

spring:
  application:
    name: agent-registry-service

redis:
  stack:
    host: 127.0.0.1
    port: 6379
    username:
    password:
    database: 0
    agent-index: agent_idx
    skill-index: skill_idx
    agent-key-prefix: "agent:"
    skill-key-prefix: "skill:"
    vector-dimension: 1536
    distance-metric: COSINE
    hnsw-m: 16
    hnsw-ef-construction: 200

dashscope:
  api-key: ${API_KEY_DASH_SCOPE}
  embedding-model: text-embedding-v2
  chat-model: qwen-plus

registry:
  heartbeat:
    ttl-seconds: 90
  health-check:
    enabled: true
    interval-seconds: 30
    timeout-ms: 2000
    failure-threshold: 3
    health-path: /lrpc/agent/health
  cleanup:
    enabled: true
    interval-seconds: 3600
    inactive-ttl-seconds: 604800
